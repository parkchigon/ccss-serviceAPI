<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    

<mapper namespace="MqttMessage">
	
	<resultMap id="MqttMessage" type="com.lgu.ccss.common.model.mqtt.MqttMessageVO">
		<result property="msgId"       column="MSG_ID" />
		<result property="msgStatus"   column="MSG_STATUS" />
		<result property="code"        column="CODE" />
		<result property="msgTitle"    column="MSG_TITLE" />
		<result property="msgCont"     column="MSG_CONT" />
		<result property="msgType"     column="MSG_TYPE" />
		<result property="recvPhoneNo" column="RECV_PHONE_NO" />
		<result property="sendType"    column="SEND_TYPE" />
		<result property="svrId"       column="SVR_ID" />
		<result property="orgNo"       column="ORG_NO" />
		<result property="callbackNo"  column="CALLBACK_NO" />
		<result property="sendDt"      column="SEND_DT" />
		<result property="regId"       column="REG_ID" />
		<result property="regDt"       column="REG_DT" />
		<result property="updId"       column="UPD_ID" />
		<result property="updDt"       column="UPD_DT" />
		<result property="expDt"       column="EXP_DT" />
		<result property="rcvReport"   column="RCV_REPORT" />
		<result property="readReport"  column="READ_REPORT" />
		<result property="sendTryCnt"  column="SEND_TRY_CNT" />
		<result property="deviceType"  column="DEVICE_TYPE" />
		
	</resultMap>
	
	<insert id="insertTbCarPushQueue" parameterType="com.lgu.ccss.common.model.mqtt.MqttMessageVO">
		INSERT INTO TB_CAR_PUSH_QUEUE (
			MSG_ID, 
			MSG_STATUS, 
			CODE, 
			MSG_TITLE,
			MSG_CONT, 
			MSG_TYPE, 
			RECV_PHONE_NO,
			SEND_TYPE,
			SVR_ID,
			<!-- ORG_NO, -->
			<!-- CALLBACK_NO, -->
			SEND_DT,
			REG_ID,
			REG_DT,
			UPD_ID,
			UPD_DT,
			EXP_DT,
			RCV_REPORT,
			READ_REPORT,
			SEND_TRY_CNT,
			DEVICE_TYPE
		)
		VALUES (
		 	#{msgId},
			#{msgStatus},
			#{code},
			#{msgTitle},
			#{msgCont},
			#{msgType},
			#{recvPhoneNo},
			#{sendType},
			#{svrId},
			<!-- #{orgNo}, -->
			<!-- #{callbackNo}, -->
			TO_DATE(#{sendDt},'yyyymmddhh24miss'),
			'SYSTEM',
			SYSDATE,
			'SYSTEM',
			SYSDATE,
			TO_DATE(#{expDt},'yyyymmddhh24miss'), 
			#{rcvReport},
			#{readReport},
			0,
			#{deviceType}
		)
	</insert>

	
</mapper>